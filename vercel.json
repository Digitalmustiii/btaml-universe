{
  "version": 2,
  "builds": [
    {
      "src": "btamluniverse_project/wsgi.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.11" }
    }
  ],
  "routes": [
    { "src": "/main/(.*)", "dest": "/public/$1" },
    { "src": "/(.*)",        "dest": "btamluniverse_project/wsgi.py" }
  ]
}
