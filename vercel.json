{
  "version": 2,
  "builds": [
    {
      "src": "btamluniverse_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "btamluniverse_project/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "btamluniverse_project/wsgi.py"
    }
  ]
}