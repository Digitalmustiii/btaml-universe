{
  "builds": [
    { "src": "staticfiles/**/*",            "use": "@vercel/static" },
    { "src": "btamluniverse_project/wsgi.py","use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/staticfiles/$1" },
    { "src": "/(.*)",        "dest": "btamluniverse_project/wsgi.py" }
  ]
}
